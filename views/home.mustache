<h1 class="is-vishidden">{{ title }}</h1>

<section class="slideshow">
	<h2 class="is-vishidden">Projekte</h2>
	<div class="slide-wrapper">
	{{# slides }}
		<div class="slide">
			<h3 class="side-headlines"><a href='{{ url }}'>{{ title }}</a></h3>
			<div class="slide-imagewrapper">
				<img src="{{ image.url }}" sizes="{{ image.sizes }}" srcset="{{ image.srcset }}" alt="{{ image.desc }}" />
			</div>
		</div>
	{{/ slides }}	
	</div>
</section>

